{
  "version": 2,
  "builds": [
    {
      "src": "backend_app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend_app.py"
    },
    {
      "src": "/(.*)",
      "dest": "0.0.0.0:8501"
    }
  ]
}
